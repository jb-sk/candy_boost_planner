{
  "name": "candy-boost-planner",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.json --noEmit && vite build",
    "preview": "vite preview",
    "generate:terms": "node scripts/generate-terms.mjs",
    "generate:tables": "node scripts/generate-pokesleep-tables.mjs",
    "generate:pokemon-db": "node scripts/generate-pokemon-db.mjs",
    "generate:master": "node scripts/generate-master.mjs",
    "generate:pokemon-master": "npm run generate:master -- --interactive",
    "generate:pokemon-names": "npm run generate:master -- --non-interactive",
    "generate:pokemon-en-names": "node scripts/generate-pokemon-en-names.mjs",
    "generate:pokemon-all": "npm run generate:master",
    "set:boost": "node scripts/set-boost-config.mjs",
    "update:all": "npm run generate:master -- --interactive && npm run generate:pokemon-en-names && npm run build",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "html-to-image": "^1.11.13",
    "vue": "^3.5.0",
    "vue-i18n": "^11.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^2.1.0"
  }
}
