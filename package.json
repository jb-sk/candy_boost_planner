{
  "name": "candy-boost-planner",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.json --noEmit && vite build",
    "preview": "vite preview",
    "generate:tables": "node scripts/generate-pokesleep-tables.mjs",
    "generate:pokemon-db": "node scripts/generate-pokemon-db.mjs",
    "generate:pokemon-master": "node scripts/generate-pokemon-master.mjs --interactive",
    "generate:pokemon-names": "node scripts/generate-pokemon-master.mjs --non-interactive",
    "generate:pokemon-all": "npm run generate:pokemon-db && node scripts/generate-pokemon-master.mjs --interactive",
    "set:boost": "node scripts/set-boost-config.mjs"
  },
  "dependencies": {
    "html-to-image": "^1.11.13",
    "vue": "^3.5.0",
    "vue-i18n": "^11.2.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vue-tsc": "^2.1.0"
  }
}
